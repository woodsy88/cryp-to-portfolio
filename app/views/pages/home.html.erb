<div class="container">
  <% for x in @coins %>
    <% for coin in @my_coins %>
        <% if coin == x["symbol"] %>
          <div class="card">
            <%= x["name"] %>
            <%= x["symbol"] %>
            $<%= x["price_usd"] %>
          </div>
        <% end %>
    <% end %>
  <% end %>
</div>